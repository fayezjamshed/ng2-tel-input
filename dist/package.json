{
  "name": "@fayezjamshed/ng2-tel-input",
  "version": "3.0.4",
  "description": "An Angular 2 wrapper for intl-tel-input library",
  "main": "ng2-tel-input.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gauravsoni119/ng2-tel-input.git"
  },
  "keywords": [
    "Angular",
    "country",
    "intl-tel-input"
  ],
  "author": "sonigaurav119@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/gauravsoni119/ng2-tel-input.git"
  },
  "homepage": "https://github.com/gauravsoni119/ng2-tel-input#readme",
  "peerDependencies": {
    "@angular/core": "^13 || ^14 || ^15 || ^16",
    "intl-tel-input": "^17.0.21"
  },
  "config": {
    "allowedNonPeerDependencies": [
      "intl-tel-input"
    ]
  },
  "module": "fesm2022/fayezjamshed-ng2-tel-input.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/fayezjamshed-ng2-tel-input.mjs",
      "esm": "./esm2022/fayezjamshed-ng2-tel-input.mjs",
      "default": "./fesm2022/fayezjamshed-ng2-tel-input.mjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.3.0"
  }
}